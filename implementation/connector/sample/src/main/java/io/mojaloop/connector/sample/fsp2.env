############################################
# FSP 2 Connector .env
#
# This file lists all environment variables read by
# io.mojaloop.connector.sample.SampleConnectorSettings
# with example values. Adjust as needed for your setup.
############################################

# =====================
# FSPIOP participant
# =====================
FSPIOP_FSP_CODE=fsp2
FSPIOP_FSP_NAME=FSP 2
FSPIOP_ILP_SECRET=ilp_secret_!!
FSPIOP_SIGN_JWS=true
FSPIOP_VERIFY_JWS=true

# Comma-separated list of other FSPs you interact with
FSPIOP_FSPS=hub,fsp1,fsp3

# Private key for this FSP (PEM, single line or quoted/multiline as supported by your loader)
FSPIOP_PRIVATE_KEY_PEM=-----BEGINPRIVATEKEY----------ENDPRIVATEKEY-----

# Public keys of other FSPs (names must match FSPIOP_FSPS)
FSPIOP_PUBLIC_KEY_PEM_OF_HUB=-----BEGINPUBLICKEY----------ENDPUBLICKEY-----
FSPIOP_PUBLIC_KEY_PEM_OF_FSP1=-----BEGINPUBLICKEY----------ENDPUBLICKEY-----
FSPIOP_PUBLIC_KEY_PEM_OF_FSP3=-----BEGINPUBLICKEY----------ENDPUBLICKEY-----

# =====================
# Inbound (server/Tomcat)
# =====================
FSPIOP_INBOUND_PORT=9090
FSPIOP_INBOUND_MAX_THREAD=200
FSPIOP_INBOUND_CONNECTION_TIMEOUT=30000

# mTLS for inbound
FSPIOP_INBOUND_USE_MUTUAL_TLS=false
# If true, also set the following
FSPIOP_INBOUND_KEYSTORE_FILE=
FSPIOP_INBOUND_KEYSTORE_BASE64=false
FSPIOP_INBOUND_KEYSTORE_PASSWORD=
FSPIOP_INBOUND_TRUSTSTORE_FILE=
FSPIOP_INBOUND_TRUSTSTORE_BASE64=false
FSPIOP_INBOUND_TRUSTSTORE_PASSWORD=

# =====================
# Outbound
# =====================
FSPIOP_OUTBOUND_PORT=9091
FSPIOP_OUTBOUND_MAX_THREAD=200
FSPIOP_OUTBOUND_CONNECTION_TIMEOUT=30000

